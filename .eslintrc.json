{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["airbnb-base"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "semi": "error",
        "no-unused-vars": "warn",
        "no-console": "off",
        "func-names": "off",
        "no-var": "error",
        "implicit-arrow-linebreak": "off",
        "linebreak-style": "off"
    }
}
